{
  "cli": {
    "version": ">= 2.5.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "beta": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk",
        "developmentClient": true
      },
      "distribution": "internal"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "preview-simulator": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {}
  }
}
